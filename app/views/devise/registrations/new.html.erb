<!-- BEGIN LOGIN -->
<div class="content">
	<!-- BEGIN REGISTRATION FORM -->
	<h3 class="form-title">Crie sua conta:</h3>

	<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
		<%= devise_error_messages! %>

		<div class="form-group">
			<!--ie8, ie9 does not support html5 placeholder, so we just show field title for that-->
			<div class="input-icon">
				<i class="fa fa-font"></i>
				<%= f.text_field :fullname,  autofocus: false, :autocomplete => "on", :class =>"form-control placeholder-no-fix", :placeholder =>"Nome Completo"%>
			</div>
		</div>

		<div class="form-group">
			<!--ie8, ie9 does not support html5 placeholder, so we just show field title for that-->
			<div class="input-icon">
				<i class="fa fa-envelope"></i>
				<%= f.email_field :email,  autofocus: false, :autocomplete => "on", :class =>"form-control placeholder-no-fix", :placeholder =>"Email"%>
			</div>
		</div>

		<div class="form-group">
			<div class="input-icon">
					<% if @validatable %>
						<em>(<%= @minimum_password_length %> characters minimum)</em>
					<% end %> <i class="fa fa-lock"></i>
					<%= f.password_field :password, autocomplete: "off", :class =>"form-control placeholder-no-fix", :placeholder =>"Senha" %>
			</div>
		</div>
		
		<div class="form-group">
			<div class="controls">
				<div class="input-icon">
					<i class="fa fa-check"></i>
						<%= f.password_field :password_confirmation, autocomplete: "off", :class =>"form-control placeholder-no-fix", :placeholder =>"Digite sua senha novamente" %>
				</div>
			</div>
		</div>

		 <div class="actions">
				<%= f.submit "Sign up", :class => "btn blue pull-right"%>
			</div><br><br>

		<% end %>
	<!-- END REGISTRATION FORM -->
</div>
