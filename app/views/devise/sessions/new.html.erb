<!-- BEGIN HEAD -->
<head>
<meta charset="utf-8"/>

<title>MASTERCOM</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta content="width=device-width, initial-scale=1.0" name="viewport"/>
<meta http-equiv="Content-type" content="text/html; charset=utf-8">
<meta content="" name="description"/>
<meta content="" name="author"/>
<link rel="shortcut icon" href="/favicon.ico"/>

</head>

<body class="login">
<!-- BEGIN LOGO -->
<div class="logo">
  <a href="">
  <img src="../../assets/admin/layout3/img/logo-big.png" alt=""/>
  </a>
</div>
<!-- END LOGO -->
<!-- BEGIN SIDEBAR TOGGLER BUTTON -->
<div class="menu-toggler sidebar-toggler">
</div>
<!-- END SIDEBAR TOGGLER BUTTON -->

<!-- BEGIN LOGIN -->
<div class="content">
  <!-- BEGIN LOGIN FORM -->
    <h3 class="form-title">Entre em sua conta</h3>
    
  
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    
      <div class="field">
        <label class="control-label visible-ie8 visible-ie9">Email</label>
        <div class="input-icon">
          <i class="fa fa-user"></i>
          <%= f.email_field :email, autofocus: true, :autocomplete => "on", :class =>"form-control placeholder-no-fix", :placeholder =>"Email" %>
        </div>
      </div><br>

      <div class="form-group">
       <div class="field">
         <label class="control-label visible-ie8 visible-ie9">Senha</label>
          <div class="input-icon">
            <i class="fa fa-lock"></i>
            <%= f.password_field :password, autocomplete: "off", :class =>"form-control placeholder-no-fix", :placeholder =>"Senha" %>
          </div>
        </div>
      </div>
      
      <div class="actions">
        <%= f.submit "Fazer Login", :class => "btn blue pull-right"%>
      </div><br><br>

    <% end %>
     

   
  <!-- END LOGIN FORM -->
</div>
</body>
<!-- END LOGIN -->

<!-- BEGIN COPYRIGHT -->
