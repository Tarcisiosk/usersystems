<!DOCTYPE html>
<html >
	<head>
		<%= stylesheet_link_tag 'application', media: 'all'%>
		<%= javascript_include_tag 'application' %>
		<%= csrf_meta_tags %>
	</head>

	<body ng-app='myApp'>
		<% flash.each do |key, value| %>
			<%= content_tag :div, value, :class => "Metronic-alerts alert alert-danger fade in" unless value.blank? %>
		<% end %>

		<% if notice %>
			
		<% end %>
			
		<%= yield %>
	</body>

	
</html>
